<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="360" prefWidth="640" spacing="10" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.flowmato.controller.SignUpController">
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
   <ImageView fitHeight="144.0" fitWidth="139.0" pickOnBounds="true" preserveRatio="true" scaleX="0.5" scaleY="0.5" translateX="-50.0">
      <image>
         <Image url="@../../../images/Flowmato.png" />
      </image>
   </ImageView>
    <Label text="Hello I'm Flowmato">
        <font>
            <Font size="40.0" />
        </font></Label>
    <TextField fx:id="emailField" promptText="Email address" />
    <TextField fx:id="passwordField" promptText="Password" />
    <TextField fx:id="preferredNameField" promptText="What can I call you?" />
    <Button onAction="#SignUpSubmit" text="Submit" />
    <Button onAction="#Back" text="Back" />
</VBox>
