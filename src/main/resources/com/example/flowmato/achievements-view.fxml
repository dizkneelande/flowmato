<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox style="-fx-background-color: linear-gradient(to bottom, #8FBCBB 30%, #ffffff 100%); -fx-padding: 20; -fx-border-radius: 10; -fx-background-radius: 10;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.flowmato.controller.AchievementsController">
    <children>
        <Label style="-fx-font-size: 30px; -fx-text-fill: black; -fx-padding: 10;" text="Achievements!" />
        <VBox fx:id="achievementsContainer" spacing="10" style="-fx-background-color: white; -fx-padding: 20; -fx-border-radius: 10; -fx-background-radius: 10;">
        </VBox>
        <StackPane alignment="BOTTOM_RIGHT">
            <ImageView fitHeight="87.0" fitWidth="73.0" pickOnBounds="true" preserveRatio="true" translateY="5.0">
                <image>
                    <Image url="@../../../images/Flowmato.png" />
                </image>
            </ImageView>
        </StackPane>
    </children>
</VBox>
