<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<StackPane prefHeight="360" prefWidth="640" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.flowmato.controller.MainApplicationController">
    <Label fx:id="currentTime" text="main app stuff goes here">
        <font>
            <Font size="40.0" />
        </font></Label>
    <Button onAction="#StartTimer" prefHeight="71.0" prefWidth="118.0" text="StartTimer" StackPane.alignment="TOP_LEFT" />
    <Button onAction="#StopTimer" prefHeight="71.0" prefWidth="118.0" text="StopTimer" StackPane.alignment="BOTTOM_LEFT" />
    <Button onAction="#PauseTimer" prefHeight="71.0" prefWidth="118.0" text="PauseTimer" StackPane.alignment="BOTTOM_RIGHT" />
</StackPane>
